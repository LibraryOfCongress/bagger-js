{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "ecmaFeatures": {
        "jsx": true
    },
    "rules": {
        "strict": [0, "function"],
        "no-use-before-define": 0,
        "no-undef": 1,
        "quotes": [1, "single", "avoid-escape"]
    }
}
