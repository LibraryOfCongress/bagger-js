{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "ecmaFeatures": {
        "jsx": true
    },
    extends: "eslint:recommended",
    "rules": {
        "strict": [0, "function"],
        "max-len": ["warn", 110, 4],
        "no-bitwise": "warn",
        "camelcase": "warn",
        "curly": "error",
        "no-extend-native": "error",
        "no-use-before-define": 0,
        "no-undef": 1,
        "quotes": [1, "single", "avoid-escape"],
        "react/jsx-uses-vars": 1,
        "indent": [1, 4, {"SwitchCase": 1}],
        "react/jsx-closing-bracket-location": 1,
        "react/jsx-indent-props": 1,
        "react/jsx-indent": 1,
        "object-shorthand": 1
    },
    "plugins": [
        "babel",
        "eslint-plugin-react"
    ]
}
